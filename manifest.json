{
    "name": "DaBorstens New Tab",
    "description": "New Tab Page",
    "author": "DaBorsten",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "storage"
    ],
    "action": {
        "default_icon": {
            "16": "/images/newTabPage16.png",
            "32": "/images/newTabPage32.png",
            "48": "/images/newTabPage48.png",
            "128": "/images/newTabPage128.png"
        }
    },
    "icons": {
        "16": "/images/newTabPage16.png",
        "32": "/images/newTabPage32.png",
        "48": "/images/newTabPage48.png",
        "128": "/images/newTabPage128.png"
    },
    "chrome_url_overrides": {
        "newtab": "NewTabPage/index.html"
    }
}